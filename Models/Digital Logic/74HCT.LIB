*pinout SOT27 1 2 3;4 5 6;9 10 8;12 13 11:VCC=14 GND=7
*pinout SO14 1 2 3;4 5 6;9 10 8;12 13 11:VCC=14 GND=7
*pinout SOT108A 1 2 3;4 5 6;9 10 8;12 13 11:VCC=14 GND=7

aand [in1 in2] out ls_and 
