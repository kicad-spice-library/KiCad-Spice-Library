.SUBCKT IIBN OUT IN VCC VEE INN GNDF
.PARAM VCMBRK = 5
EVCCTEST NTEST 0 VALUE = {SGN(V(VCC,VEE)) - 1 + ABS(SGN(V(VCC,VEE)))}
E1ACT N1SGN 0 VALUE = {SGN(V(INN)-VCMBRK) - 1 + ABS(SGN(V(INN)-VCMBRK))}
GOUT OUT IN VALUE = {0.5*(1+V(N1SGN))*0.5*(1+V(NTEST))*65u}
.ENDS
