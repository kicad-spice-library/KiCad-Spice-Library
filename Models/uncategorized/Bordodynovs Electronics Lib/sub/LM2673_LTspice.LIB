.SUBCKT LM2673_TRANS VIN FB CB VSW CURRENT_ADJ VGND SOFTSTART PARAMS: RFB1=1e6 RFB2=1e-6
*E_BSTARTUP ENABLE GND value = { IF(V(VIN,GND) > 7.90 , 5 , 0)}
BE_BSTARTUP GND ENABLE I=IF(V(VIN,VGND) > 7.90 , 5 , 0) Rpar=1
*E_B12V REFV VGND value = { IF(V(VIN,GND) > 7.90 , 1.215 , 0)}
BE_B12V GND REFV I=IF(V(VIN,VGND) > 7.90 , 1.215 , 0)*V(FBP) Rpar=1
*E_BREF REF5V GND value = { IF(V(VIN,GND) > 7.90 , 5 , 0)}
BE_BREF GND REF5V I=IF(V(VIN,VGND) > 7.90 , 5 , 0) Rpar=1
*G_BSSClamp GND SOFTSTART value = { IF(V(VIN) > 3.3 & V(SOFTSTART) < 5 , 3.7E-6 , 0 )}
BG_BSSClamp VGND SOFTSTART I=IF (V(VIN,VGND)>3.3&V(SOFTSTART,VGND)<5,3.7E-6,0)
*CSS SOFTSTART GND 26.37P rpar=1e8
CSS SOFTSTART VGND 26.37p
*E_BSSCL 361 GND value = { IF(V(101) > 1.1 , 0 , 5  )}
BE_BSSCL VGND 361 I=IF(V(101) > 1.1 , 0 , 5  ) Rpar=1
DSS SOFTSTART 361 DSS
DSS1 361 SOFTSTART DSS
.MODEL DSS D RS=10k
*XSW2 31 30 VSW NATIONALFET3A_0
XSW2 31 30 VSW VGND NATIONALFET3A_1
X_H1 VIN 31 HS_CUR_INT GND STARTUP_H1
R58 HS_CUR_INT HS_CUR 1
C58 HS_CUR 0 10n
BE_BVRSEN GND 102 I=IF( V(HS_CUR) > V(CL) , 5 , 0) Rpar=1
BE_BFS2 GND 103 I=IF( V(HS_CUR) > 1.18*V(CL) , 5-(V(HS_CUR)*.12*.3)  , 5) Rpar=1
VICL CURRENT_ADJ VGND DC=1.21
BE_BICL GND CL I=IF(37125 * -1* I(VICL) / 1.21 > 4.6 , 4.6  , 37125 * -1 * I(VICL) / 1.21 ) Rpar=1
BE_CBB GND VIN_INT I=V(VIN,VGND) Rpar=1
X4 VIN_INT CB REG1MA_0
X1 VSW CB LZEN10_0
RREG 50 52 0.33
CREF 52 GND 1N
V3 REF5V 50 0
BFBP 0 FBP I=Limit(-1,1,If(V(FB,GND)>0,1,-1)) Rpar=1
RFB2 FB EA {RFB2}
RFB1 EA VGND {RFB1}
BEA 0 EA1 I=V(EA,VGND)*V(FBP) Rpar=1
GM1 GND 15 EA1 REFV 6.73E-4
*G_BGM2 GND 17 value = {V(REFV,15) * V(13)}
BG_BGM2 GND 17 I=(V(REFV,15) * V(13))*V(FBP)
XA1 VIN_INT 13 PWL2
D1 17 REF5V _D
D2 400 17 _D
VD2 400 GND 0.9
.MODEL _D D
R3 REFV 16 10K
R4 15 16 2K
L1 16 REFV 20MH
R5 17 18 15K
C1 18 GND 10NF IC = 2.5
C6 17 18 105P
R10 17 GND 300K
E1 ErrOut GND 17 GND 0.8
*E_BSSERR 360 GND value = { IF(V(SOFTSTART) < V(ErrOut) , V(SOFTSTART) , V(ErrOut) )}
BE_BSSERR GND 360 I=IF(V(SOFTSTART,VGND) < V(ErrOut) , V(SOFTSTART,VGND) , V(ErrOut) ) Rpar=1
X3 RTCT 360 COMPOUT COMP_BASIC_GEN
S1 26 GND RTCT GND _S1
*.MODEL _S1 vswitch VT=1.9 VH=1.3 RON=.01 ROFF=1MEG
.MODEL _S1 sw VT=1.6 VH=300e-3 RON=.01 ROFF=1MEG
BG_BDISCH RTCT GND I=IF(V(26) < 0.6 & V(REF5V) > 3.2 , .46M , 0)
*G_BDISCH RTCT GND value={IF(V(26) < 0.6 & V(REF5V) > 3.2 , .46M , 0)}
RPULL 26 REF5V 100K
*RT RTCT 33 100K
* RT=530K=260KHz
RT RTCT 33 530K
CT RTCT 9 .0399N
VM 9 GND 0
BE_BVM GND PULSE300N I=IF(I(VM) < 0 , 1 , 0) Rpar=1
R103 103 33 200
*C103 33 GND 1N IC=5
C103 33 GND 1N
*R101 102 101 200
*C101 101 GND 1N
R101 102 201 200
C101 201 GND 1N
BP5V 0 PULSE300N5V I=V(PULSE300N)*5 Rpar=1
A1 201 PULSE300N5V 0 0 0 0 101 0 SRFLOP tau=20n Vhigh=5
X2 35 REF5V 32 GND QB Q FFLOP_0
BE_BR GND 32 I=IF(V(COMPOUT) > 2 , 5 , IF( V(101) > 1 , 5 , 0)) Rpar=1
BE_BS GND 35 I=IF(V(PULSE300N) < .5 , 5 , 0) Rpar=1
*E_BD 30 VSW value = { IF(V(Q) > 2.5 & V(SOFTSTART) > .63 & V(ENABLE) > 1.4 & V(PULSE300N) <.5 , 10 , 0)}
BE_BD VSW 30 I=IF(V(Q) > 2.5 & V(SOFTSTART,VGND) > .63 & V(ENABLE) > 1.4 & V(PULSE300N) <.5 , 5 , 0) Rpar=1

