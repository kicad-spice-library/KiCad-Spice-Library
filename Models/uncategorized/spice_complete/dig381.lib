+         DATA & TRN_LH, DELAY(3ns,5.5ns,8ns),
+         DATA & TRN_HL, DELAY(3.5ns,6ns,8.5ns),
+         DELAY(6.5ns,9.5ns,13.5ns))}
+
+     GBAR = {
