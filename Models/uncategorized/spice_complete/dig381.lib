+   HOLDTIME = 8NS
+ SETUP_HOLD:
+   CLOCK LH = S2
+   DATA(8) = D0 D1 D2 D3 D4 D5 D6 D7
+   HOLDTIME = 8NS
+ SETUP_HOLD:
+   CLOCK LH = STB
+   DATA(8) = D0 D1 D2 D3 D4 D5 D6 D7
+   HOLDTIME = 8NS

