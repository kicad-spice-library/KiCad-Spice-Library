+         CARRY & TRN_HL, DELAY(-1,13ns,23ns),
+         DELAY(-1,39ns,58ns))}
+
+     Cn4 = {
+       CASE(
+         SELECT & TRN_LH, DELAY(-1,38ns,57ns),
