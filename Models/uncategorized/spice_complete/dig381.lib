+    DC2 = {~((~A2 & XNOR2) | (~A2 & C2BARO) | (XNOR2 & C2BARO))}
+    DC3 = {~((~A3 & XNOR3) | (~A3 & C3BARO) | (XNOR3 & C3BARO))}
+    DC4 = {~((~A4 & XNOR4) | (~A4 & C4BARO) | (XNOR4 & C4BARO))}
+    DS1 = {XNOR1 ^ (~(~A1 ^ C1BARO))}
+    DS2 = {XNOR2 ^ (~(~A2 ^ C2BARO))}
+    DS3 = {XNOR3 ^ (~(~A3 ^ C3BARO))}
+    DS4 = {XNOR4 ^ (~(~A4 ^ C4BARO))}

