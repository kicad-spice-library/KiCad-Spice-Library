+     TRN_LH, DELAY(-1,15NS,30NS),
+     TRN_HL, DELAY(-1,25NS,30NS),
+     DELAY(-1,26NS,36NS))}
+ PINDLY:
+  Q3 = {
+   CASE(
+     CLEAR & TRN_HL, DELAY(-1,22NS,35NS),
+     TRN_LH, DELAY(-1,15NS,30NS),
+     TRN_HL, DELAY(-1,25NS,30NS),
+     DELAY(-1,26NS,36NS))}

U4 CONSTRAINT(7) DPWR DGND
+ P0 P1 P2 P3 CPBAR S DS
+ IO_LS IO_LEVEL={IO_LEVEL}
+ FREQ:
+  NODE = CPBAR
+  MAXFREQ = 45MEG
+ WIDTH:
+  NODE = CPBAR
+  MIN_LO = 16NS
+  MIN_HI = 16NS
+ SETUP_HOLD:
+  CLOCK HL = CPBAR
+  DATA(5) = DS P0 P1 P2 P3
+  SETUPTIME = 20NS
+  HOLDTIME = 10NS
+ SETUP_HOLD:
+  CLOCK HL = CPBAR
+  DATA(1) = S
+  SETUPTIME = 40NS

