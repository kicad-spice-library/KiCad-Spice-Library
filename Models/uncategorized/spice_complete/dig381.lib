+  AND0TOP = {~CBIBAR & LDBAR & ~UDBAR}
+  AND0BOT = {~CBIBAR & LDBAR & UDBAR}
+  AND1TOP = {AND0TOP & Q0O & LDBAR & ~UDBAR}
+  AND1BOT = {AND0BOT & Q0BARO & LDBAR & UDBAR}
+  AND2TOP = {AND1TOP & Q1O & LDBAR & ~UDBAR}
