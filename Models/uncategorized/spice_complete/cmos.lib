+          DATA & TRN_HL, DELAY(-1,520NS,1040NS),
+          DELAY(-1,661NS,1321NS))}

U4 CONSTRAINT(5) VDD VSS
+ BCDD BCDC BCDB BCDA LE
+ IO_4000B IO_LEVEL={IO_LEVEL}
+ SETUP_HOLD:
+   CLOCK HL = LE
+   DATA(4) = BCDD BCDC BCDB BCDA
+   SETUPTIME = 75NS
+ WIDTH:
+   NODE = LE
+   MIN_LO = 200NS
+   MIN_HI = 200NS

.ENDS CD4511B

