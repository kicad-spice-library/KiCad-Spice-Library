+     S9O = {(NOR7 & NOR2 & (~INH))}
+     S10O = {(NOR7 & NOR3 & (~INH))}
+     S11O = {(NOR7 & NOR4 & (~INH))}
+     S12O = {(NOR8 & NOR1 & (~INH))}
+     S13O = {(NOR8 & NOR2 & (~INH))}
