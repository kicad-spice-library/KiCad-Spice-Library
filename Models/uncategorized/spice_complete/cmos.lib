+        (BCD3 & (~BCD2) & (~BCD1)) | ((~BCD3) & BCD2 & (~BCD1) & BCD0)}
+ EX = {((~BCD0) & ((~BCD2) | ((~BCD3) & BCD1))) | (BCD3 & (BCD1 ^ BCD2))}
+ FX = {((~BCD1) & ((~BCD0) | BCD2)) | (BCD3 & (~BCD2)) |
+        ((~BCD3) & BCD2 & (~BCD0))}
+ GX = {((~BCD1) & (BCD2 | BCD3)) | (BCD1 & (((~BCD2) & BCD0) |
+        ((~BCD3) & (~BCD0)))) | (BCD2 & (~BCD0))}
