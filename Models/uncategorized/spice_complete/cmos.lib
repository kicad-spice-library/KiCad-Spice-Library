+                 TWCLKHMN=-1 TWCLKHTY=125NS TWCLKHMX=500NS
+                 TSUDCLKMN=-1 TSUDCLKTY=40NS TSUDCLKMX=200NS
+                 TSUPCCLKHMN=-1 TSUPCCLKHTY=100NS TSUPCCLKHMX=400NS)

U7 PINDLY(5,0,0) VDD VSS
