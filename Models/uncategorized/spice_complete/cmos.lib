+         MODE1 & TRN_LH, DELAY(-1,375NS,960NS),
+         MODE1 & TRN_HL, DELAY(-1,430NS,1200NS),
+         MODE2 & TRN_LH, DELAY(-1,255NS,640NS),
+         MODE2 & TRN_HL, DELAY(-1,280NS,750NS),
