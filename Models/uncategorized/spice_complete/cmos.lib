+        (TRN_Z$ | TRN_$Z), DELAY(-1,95NS,190NS),
+        SEL & (TRN_LH | TRN_HL), DELAY(-1,190NS,380NS),
+        DATA & (TRN_LH | TRN_HL), DELAY(-1,150NS,300NS),
+        DELAY(-1,191NS,381NS))}

.ENDS CD40257B

