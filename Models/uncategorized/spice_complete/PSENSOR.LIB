*HELP issyntax.hlp "Pressure Sensor Models"
.SUBCKT SPX50A  4  6  2  1  5 {SENS=8.2M TD=1M VOS=20M RIN=500 ROUT=1600}
*con           P1 Vs V+ V- Gnd
R7 6 3 {RIN}
R8 8 5 {RIN}
R9 6 10 {RIN}
R10 11 5 {RIN}
B3 10 11 V = {SENS} * (V(13) - V(14)) + {VOS}
B4 8 3 V = {SENS} * (V(13) - V(14)) + {VOS}
R11 1 3 {(ROUT-RIN)/2}
R12 2 10 {(ROUT-RIN)/2}
R13 4 9 1E4
C1 9 5 {TD/(2.3*1E4)}
E1 5 13 9 5 -1
R14 7 12 1E4
C2 12 5 {TD/(2.3*1E4)}
E2 5 14 12 5 -1
VA 7 0 DC 0.0005 ; 0.00034 kPa chanber
.ENDS
